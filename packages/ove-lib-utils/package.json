  "files": [
    "lib/*"
  ],
  "keywords": [
    "OVE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/ove/ove",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ove/ove.git"
  },
  "author": "Senaka Fernando <senaka.fernando15@imperial.ac.uk>",
  "license": "MIT",
  "dependencies": {
    "chalk": "2.4.2",
    "dateformat": "3.0.3",
    "express": "4.16.4",
    "then-request": "6.0.0",
    "http-status-codes": "1.3.0",
    "jquery": "3.3.1",
    "js-yaml": "3.12.1",
    "swagger-ui-express": "4.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**.js",
      "!*/constants.js"
    ],
    "coverageReporters": [
      "json-summary",
      "text"
