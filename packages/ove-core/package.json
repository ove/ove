{
  "name": "@ove/ove-core",
  "private": true,
  "version": "0.6.2",
  "description": "The Core library of OVE",
  "main": "dist/index.js",
  "scripts": {
    "clean": "npx rimraf dist",
    "build": "npx babel src --out-dir dist --copy-files",
    "build:windows": "npx babel-cli src --out-dir dist --copy-files",
    "test": "jest --runInBand --coverage --colors",
    "start": "node dist/index.js --color",
    "start:dev": "babel-node src/index.js",
    "lint": "eslint src/. --ext .js"
  },
  "keywords": [],
  "author": "Senaka Fernando <senaka.fernando15@imperial.ac.uk>",
  "license": "MIT",
  "dependencies": {
    "@ove-lib/utils": "^0.5.0",
    "axios": "0.21.4",
    "cors": "2.8.5",
    "d3": "6.7.0",
    "datatables.net-dt": "1.10.24",
    "express": "4.17.1",
    "express-ws": "4.0.0",
    "github-markdown-css": "4.0.0",
    "http-status-codes": "2.1.4",
    "jquery": "3.6.0",
    "request": "2.88.2",
    "uglify-js": "3.13.7",
    "ws": "7.5.3"
  },
  "devDependencies": {
    "mock-socket": "9.0.3",
    "nock": "13.0.11"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/server/*.js"
    ],
    "coverageReporters": [
      "json-summary",
      "text"
    ]
  }
}
