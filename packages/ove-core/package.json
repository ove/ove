{
  "name": "@ove/ove-core",
  "private": true,
  "version": "0.2.0",
  "description": "The Core library of OVE",
  "main": "dist/index.js",
  "scripts": {
    "clean": "npx rimraf dist",
    "build": "npx babel-cli src --out-dir dist --copy-files",
    "test": "jest --coverage --colors",
    "start": "node dist/index.js --color",
    "start:dev": "babel-node src/index.js",
    "lint": "eslint src/. --ext .js"
  },
  "keywords": [],
  "author": "Senaka Fernando <senaka.fernando15@imperial.ac.uk>",
  "license": "MIT",
  "dependencies": {
    "@ove-lib/utils": "^0.2.0",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-ws": "4.0.0",
    "http-status-codes": "1.3.0",
    "request": "2.88.0",
    "uglify-js": "3.4.9",
    "github-markdown-css": "3.0.0"
  },
  "devDependencies": {
    "mock-socket": "8.0.5",
    "nock": "10.0.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/server/*.js"
    ],
    "coverageReporters": [
      "json-summary",
      "text"
    ]
  }
}
